{
    "name": "fmpocket",
    "version": "1.2.2",
    "description": "A lightweight universal typed client for the Financial Modeling Prep (FMP) API. Gets your financial data perfectly formatted.",
    "author": {
        "name": "Louis Grange",
        "url": "https://github.com/l0uisgrange"
    },
    "license": "MIT",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/l0uisgrange/fmpocket.git"
    },
    "keywords": [
        "fmp",
        "financial",
        "stock",
        "data",
        "isomorphic",
        "universal"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsdown",
        "lint": "oxlint --vitest-plugin",
        "test": "vitest --run",
        "check:format": "oxfmt --check **/*",
        "format": "oxfmt **/*"
    },
    "devDependencies": {
        "@types/node": "^24.10.1",
        "oxfmt": "^0.14.0",
        "oxlint": "^1.29.0",
        "tsdown": "^0.15.12",
        "typescript": "^5.9.3",
        "vitest": "^4.0.13"
    },
    "dependencies": {
        "zod": "^4.1.13"
    }
}
